{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"types": ["node"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"allowJs": true,
		"outDir": "dist",
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["./src/**/*"]
}
